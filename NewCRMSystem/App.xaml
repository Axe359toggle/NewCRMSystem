<Application x:Class="NewCRMSystem.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:System="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:NewCRMSystem"
             StartupUri="Login.xaml">
    <Application.Resources>
        <BitmapImage x:Key="backButtonImage" UriSource="/Resources/icons8_Back_To_96px.png" />
        <BitmapImage x:Key="notifyCorrectImage" UriSource="/Resources/icons8_Checked_96px.png" />
        <BitmapImage x:Key="notifyErrorImage" UriSource="/Resources/icons8_High_Priority_96px.png" />
        <BitmapImage x:Key="LogoImage" UriSource="/Resources/icons8_High_Priority_96px.png" />
        <System:Double x:Key="panelSize">50</System:Double>
        <GridLength x:Key="panelRowSize">50</GridLength>
    </Application.Resources>
</Application>
